<%- include("layout/header") %>
<main>
  <form action="/" method="post">
    <input
      type="text"
      placeholder="Add a Programming language"
      name="pLanguage"
    />
    <button type="submit">Add</button>
  </form>
  <ul>
    <% if(plNames.length > 0){ %> <%for(let x =0; x < plNames.length; x++){ %>
    <li><%= plNames[x] %></li>
    <%}%> <% }else{ %>
    <p>Not Found</p>
    <% } %>
  </ul>
</main>
<%- include("layout/footer") %>
